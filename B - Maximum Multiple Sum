By eshan07, contest: Codeforces Round 952 (Div. 4), problem: (B) Maximum Multiple Sum, Accepted, #, Copy
#include<iostream>
using namespace std;
 
int main()
{
    int t;
    cin>>t;
 
    while(t--)
    {
        int n;
        cin>>n;
 
        if(n==3)
        {
            cout<<"3"<<endl;
        }
        else
        {
            cout<<'2'<<endl;
        }
    }
}
