#include<iostream>
using namespace std;
int main()
{
    int a,i;
   int count =0;
    cin>>a;
    int arr[a];

    for(i=0; i<a; i++)
    {
        cin>>arr[i];
        if(arr[i]==1)
        {
            count++;
        }
    }
    if(count==0)
    {
        cout<<"EASY";
    }
    else
    {
        cout<<"HARD";
    }
}
