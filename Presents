#include<iostream>
using namespace std;
int main()
{
    int a;
    cin>>a;
    int array[a];

    for(int i=0; i<a; i++)
    {
        cin>>array[i];
    }

    for(int j=1; j<=a; j++)
    {
        for(int i=0; i<a; i++)
        {
            if(array[i]==j)
            {
                cout<<i+1<<" ";
            }
        }
    }
}
