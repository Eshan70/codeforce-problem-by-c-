#include <bits/stdc++.h>
using namespace std;

int main() {
    int test_case;
    cin >> test_case;

    while (test_case--) {
        int n, k;
        cin >> n >> k;
        vector<int> a(n);
        bool eshan = true;

        for (int i = 0; i < n; i++) {
            cin >> a[i];
        }

        if (k>=2) {
            cout<<"YES"<<endl;

        }

        else {
                vector<int>p=a;
                sort(p.begin(),p.end());
                if(a==p) {
                        cout<<"YES"<<endl;
                }
                else {
                        cout<<"NO"<<endl;
                }
        }

    }

}
