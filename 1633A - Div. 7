#include <iostream>
using namespace std;
 
int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
 
       int vagses = n%7;
        int y = 7-vagses;
        int modulo = n%10;
        if(n%7==0)
       n=n;
        else if(modulo+y<10)
                n+=y;
        else n-=vagses;
        cout<<n<<endl;
        }
    }
