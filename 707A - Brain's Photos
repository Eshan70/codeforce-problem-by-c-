#include <iostream>
using namespace std;
 
int main() {
    int n, m;
    cin >> n >> m;
    bool Color = false;
 
 
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            char p;
            cin >> p;
 
 
            if (p == 'C' || p== 'M' || p == 'Y') {
                Color = true;
            }
        }
    }
 
 
    if (Color) {
        cout << "#Color" << endl;
    } else {
        cout << "#Black&White" << endl;
    }
 
    return 0;
}
