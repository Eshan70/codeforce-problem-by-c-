#include<iostream>
#include<string>
using namespace std;
 
int main()
{
    int i,t;
    cin>>t;
 
    while(t--)
    {
        string s;
        cin>>s;
        int l=s.size();
 
        if(l%2==0)
        {
            string s1,s2;
            for(i=0; i<l/2; i++)
            {
               s1+=s[i];
            }
            for(i=l/2; i<l; i++)
            {
                 s2+=s[i];
            }
            if(s1==s2)
            {
                cout<<"YES"<<endl;
            }
            else
            {
                cout<<"NO"<<endl;
            }
 
        }
        else
        {
            cout<<"NO"<<endl;
        }
 
        }
    }
