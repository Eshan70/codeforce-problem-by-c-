#include<bits/stdc++.h>
using namespace std;
int main() {
int t;
cin>>t;
while(t--) {
        int d;
        cin>>d;
        int a,b,c;
        cin>>a>>b>>c;
       if((d==1 && a==0) || (d==2 && b==0) || (d==3 && c==0) )
       {
               cout<<"NO"<<endl;
       }
       else if ( (d==1 && a==2 && b==3) || (d==1 && a==3 && c==2) ) {
                       cout<<"YES"<<endl;
       }
 
       else if ( (d==2 && b==1 && a==3) || (d==2 && b==3 && c==1) ) {
                       cout<<"YES"<<endl;
       }
 
        else if ( (d==3 && c==1 && a==2) || (d==3 && c==2 && b==1) ) {
                       cout<<"YES"<<endl;
       }
       else {
        cout<<"NO"<<endl;
       }
 
}
}
