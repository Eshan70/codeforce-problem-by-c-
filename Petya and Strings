#include<iostream>
#include<string>
using namespace std;
int main()
{
 int i;
 string a,b;
 cin>>a>>b;
 int n=a.size();
 for(i=0; i<n; i++)
 {
     a[i]=towlower(a[i]);
     b[i]=towlower(b[i]);
 
 }
 if(a==b)
 {
     cout<<"0"<<endl;
 }
 else
    for(i=0; i<n; i++)
 {
     if(a>b)
     {
         cout<<"1"<<endl;
         break;
     }
     if(a<b)
     {
         cout<<"-1"<<endl;
         break;
     }
 }
 
 
}
