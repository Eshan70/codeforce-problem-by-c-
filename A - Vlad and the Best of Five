#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int t;
    cin>>t;
 
    while(t--)
    {
      string s;
      cin>>s;
      int count=0,count1=0;
 
      for(int i=0; i<s.size(); i++)
      {
          if(s[i]=='A')
          {
              count++;
          }
          else if(s[i]=='B')
          {
              count1++;
          }
      }
      if(count>count1)
      {
          cout<<"A"<<endl;
      }
      else
      {
          cout<<"B"<<endl;
      }
    }
}
