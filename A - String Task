#include <bits/stdc++.h>
#include <string>
using namespace std;
 
int main() {
    string s;
    cin >> s;
 
string s1;
 
 
    for (int i=0; i<s.size();i++) {
       char c= tolower(s[i]);
       if(c=='a' || c=='e' || c=='i' || c=='o' || c=='u'|| c=='y')
       {
               continue;
       }
       else
       {
                s1='.';
               s1+=c;
              cout<<s1;
 
       }
    }
}
