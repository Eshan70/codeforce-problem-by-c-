#include<iostream>
#include<string>
using namespace std;
int main()
{
    int n,sum1=0,sum2=0;
    cin>>n;
    string s;
    cin>>s;
    int i;
    for(i=0; i<n; i++)
    {
        if(s[i]=='A')
            sum1++;
        else if(s[i]=='D')
            sum2++;
    }
    if(sum1>sum2)
    {
        cout<<"Anton";
    }
        else if(sum1<sum2)
            {
            cout<<"Danik";
            }
        else if(sum1==sum2)
        {
            cout<<"Friendship";
        }
 
 
}
