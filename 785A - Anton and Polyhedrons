#include<iostream>
#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int t,i;
    cin>>t;
    string s[t];
   int count=0;
 
    for(i=0; i<t; i++)
    {
 
        cin>>s[i];
 
        if(s[i]=="Tetrahedron")
        {
            count+=4;
        }
        else if(s[i]=="Cube")
        {
            count+=6;
        }
        else if(s[i]=="Octahedron")
        {
            count+=8;
        }
        else if(s[i]=="Dodecahedron")
        {
            count+=12;
        }
        else if(s[i]=="Icosahedron")
        {
            count+=20;
        }
    }
    cout<<count;
}
