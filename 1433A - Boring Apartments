#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        int n=s.size();
        string a ;
        a+=s[0];
        int b = stoi(a);
        int c=(b- 1)*10;
        int A=0;
        for(int i=1;i<=n;i++){
            A+=i;
        }
        cout<<A+c<<endl;
    }
}
