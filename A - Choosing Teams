#include<iostream>
using namespace std;
int main()
{
    int i,n,k;
    cin>>n >>k;
    int arr[n];
    int count =0;
    for(i=0; i<n; i++)
    {
        cin>>arr[i];
        if((5-arr[i])>=k)
        {
            count++;
        }
    }
    //if(count<=3)
    //{
        cout<<count/3;
    //}
   // else
   // {
        //cout<<'0';
   // }
 
}
