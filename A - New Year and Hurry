#include<iostream>
using namespace std;
 
int main() {
int n,m;
cin>>n>>m;
int left=240-m;
int p=5*(n*(n+1)/2);
int q=0;
int count=-1;
if(p>left)
{
    for(int i=1;;i++)
    {
        if(q<=left)
        {
            q=q+5*i;
            count++;
        }
        else
        {
        break;
        }}
        cout<<count;
        
    }
    else
    {
        cout<<n;
    }
}
