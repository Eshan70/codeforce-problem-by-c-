#include <iostream>
using namespace std;
 
int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, k;
        cin >> n >> k;
 
        int a[n];
        bool eshan = false;
 
        for (int i = 0; i < n; i++) {
            cin >> a[i];
            if (a[i] == k) {
                eshan = true;
            }
        }
 
        if (eshan) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
    return 0;
}
