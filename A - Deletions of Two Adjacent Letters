#include <iostream>
#include <string>
using namespace std;
 
int main() {
    int t;
    cin >> t;
    while (t--) {
        string s;
        cin >> s;
        char c;
        cin >> c;
 
        bool eshan = false;
 
 
        for (int i = 0; i < s.size(); i += 2) {
            if (s[i] == c) {
                eshan = true;
                break;
            }
        }
 
 
        if (eshan) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
 
 
}
