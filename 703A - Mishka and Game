#include<iostream>
using namespace std;
 
int main() {
 
int n,m,c,i;
int count=0;
int count1=0;
 
cin>>n;
 
for(i=1; i<=n; i++)
{
    cin>>m>>c;
    if(m>c)
    {
      count++;
    }
    if(m<c)
    {
        count1++;
    }
 
 
 
}
if(count>count1)
{
    cout<< "Mishka";
}
if(count<count1)
{
    cout<<"Chris";
}
if(count==count1)
{
    cout<<"Friendship is magic!^^";
}
 
 
}
